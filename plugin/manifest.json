{
    "manifest_version": 2,
    "name": "Disable Katex to Mathjax",
    "version": "1.0",
    "description": "Disable katex rendering and display Mathjax source code.",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }

  